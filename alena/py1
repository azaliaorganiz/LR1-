import copy
import math

a: int = 2
b: float = 0.1
suAB = a + b
print("Сумма: ", suAB)
razAB = a - b
print("Разность a b: ", razAB)
delAB = a / b
print("Тип при делении : ", type(delAB))
print("\b")
r:float = 5.0
pi:float = math.pi
s = pi * math.pow(r,2)
print(F"Площадь круга до округления: {s}")
sOkr = round(s,2)
print(F"Площадь круга после округления: {sOkr}")
